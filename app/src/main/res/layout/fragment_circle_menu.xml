<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@color/light_primary_color"
    tools:context="com.wangenyong.weytest.fragments.CircleMenuFragment">

    <com.wangenyong.mylibrary.views.CircleMenuLayout
        android:id="@+id/layout_circle_menu"
        android:layout_width="240dp"
        android:layout_height="240dp"
        android:layout_centerInParent="true"
        android:padding="8dp"
        android:background="@drawable/bg_circle_menu">

        <android.support.design.widget.FloatingActionButton
            android:id="@+id/id_circle_menu_item_center"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:src="@drawable/ic_action_android"
            app:backgroundTint="@color/accent_color"
            app:fabSize="normal"
            app:elevation="4dp"
            app:borderWidth="0dp"
            android:clickable="true"/>

    </com.wangenyong.mylibrary.views.CircleMenuLayout>

</RelativeLayout>
